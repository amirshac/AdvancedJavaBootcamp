package VendingMachine.VendingMachine;

public class Soda extends ColdDrink{
	
	protected boolean isCanned;
	
	// constructors
	public Soda() {
		super("Soda", 100, 7, 2);
		this.ingridient = 0;
		isCanned = true;
	}
	
	public Soda(String name) {
		super(name);
	}
	
	@Override
	public void produce() {
		super.produce();
	}
}
