package javabootcamp.oopexam.ToyPlayedByKid;

import javabootcamp.oopexam.Kid.Kid;
import javabootcamp.oopexam.Toy.Toy;

public class ToyPlayedByKid {
	private Toy toy;
	private Kid kid;
	
	public ToyPlayedByKid(Toy toy, Kid kid) {
		this.toy = toy;
		this.kid = kid;
	}
	
	public ToyPlayedByKid() {
		this.toy = null;
		this.kid = null;
	}

	public Toy getToy() {
		return toy;
	}

	public void setToy(Toy toy) {
		this.toy = toy;
	}

	public Kid getKid() {
		return kid;
	}

	public void setKid(Kid kid) {
		this.kid = kid;
	}
	
	
}
