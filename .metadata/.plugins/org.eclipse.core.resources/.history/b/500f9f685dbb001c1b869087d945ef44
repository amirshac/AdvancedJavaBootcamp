package Library;

public abstract class Book {
	private int quantity;
	private String name;
	
	
	public Book(String name, int quantity, BookType bookType) {
		this.name = name;
		this.quantity = quantity;
		this.bookType = bookType;
	}
	
	@Override
	public String toString() {
		String msg = new String();
		msg += String.format("<Book> Name: %s, Quantity: %d%n");
		return msg;
	}

}

enum BookType{
	COOKING,
	NOVEL,
	DICTIONARY
}
